<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.nowon.domain.dao.QnaMapper">
	<!-- 1. !DOCTYPE mapper 정의되어있어야함 -->
	<!-- 2. namespace는 @Mapper 인터페이스 와 일치해야 함 -->
	<!-- 3. id는 @Mapper 인터페이스의 메서드이름 과 일치해야 함 -->
	<!-- 4. select태그는  resultType 정의해서 resultMapping함 -->
	
	<insert id="save">
		insert into board(title, content, writer, email) values(#{title},#{content},#{writer},#{email})
	</insert>

</mapper>